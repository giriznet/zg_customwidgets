{
	"id": "com.sap.sample.ZG_NUMCHART2",
	"version": "2.0.0",
	"name": "ZG_NUMCHART2",
	"description": "Giri's Customized NUMCHART2",
	"newInstancePrefix": "ZG_NUMCHART2",
	"icon": "https://github.com/giriznet/zg_customwidgets/blob/main/ZG_NUMCHART2.PNG",
	"vendor": "GIRI-2",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sap-sample-ZG_NUMCHART2",
			"url": "https://github.com/giriznet/zg_customwidgets/blob/main/ZG_NUMCHART2.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"color": {
			"type": "string",
			"description": "Background color",
			"default": "red"
		},
		"opacity": {
			"type": "number",
			"description": "Opacity",
			"default": 1
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},
	"methods": {
		"setColor": {
			"description": "Sets the background color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "The new background color"
				}
			],
			"body": "this.color = newColor;"
		},
		"getColor": {
			"returnType": "string",
			"description": "Returns the background color.",
			"body": "return this.color;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the ZG_NUMCHART2."
		}
	}
}

 
